<?xml version="1.0"?>
<launch>
	<!-- arguments -->
	<arg name="prefix" default="test" />
	<arg name="imu_topics" default="/imu/data" doc="" />
	<arg name="camera_topics" default="/camera_F/color/camera_info /camera_F/detph/camera_info /camera_F/color/image_raw /camera_F/depth/image_rect_raw" doc="" />
	<arg name="soma_topics" default="/soma/wheel_vel /soma/wheel_odometry /soma/action /soma/action_str" doc="" />
	<arg name="topics" default="/velodyne_points $(arg imu_topics) $(arg camera_topics) $(arg soma_topics) /maxon_bringup/get_all_states /maxon_bringup/steering/get_state /maxon_bringup/rear_brake/get_state /maxon_brinup/front_state /maxon_bringup/throttle/get_state" />
	<!--  -->
	<node pkg="rosbag" type="record" name="soma_logging" output="screen" args="-o $(arg prefix) $(arg topics) tf tf_static">
	</node>
</launch>
